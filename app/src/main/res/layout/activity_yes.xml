<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:tag="cards main container"
    android:id="@+id/cid">

    <LinearLayout
        android:id="@+id/container_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <include
            android:id="@+id/toolbar"
            layout="@layout/toolbar" />

    </LinearLayout>

    <android.support.v7.widget.CardView
        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:id="@+id/card_view"
        android:layout_width="match_parent"
        android:layout_height="270dp"
        android:layout_marginTop="10dp"
        android:paddingTop="@dimen/title_50dp"
        card_view:cardBackgroundColor="#ffffff"
        card_view:cardCornerRadius="@dimen/dp10"
        card_view:cardElevation="5dp"
        card_view:cardUseCompatPadding="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">


            <!--ImageView
                android:id="@+id/imageView"
                android:tag="image_tag"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:layout_weight="1"
               /-->

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="200dp"
                android:layout_marginTop="10sp"
                android:layout_weight="2"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textViewName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="10dp"
                    android:text="Are You New User ?"
                    android:textSize="20sp" />


                <Button
                    android:id="@+id/button1"
                    android:layout_width="144dp"
                    android:layout_height="50dp"
                    android:layout_gravity="center"
                    android:layout_margin="40dp"
                    android:background="@android:drawable/dialog_holo_light_frame"
                    android:backgroundTint="#f8a519"
                    android:fontFamily="HelveticaNeue"
                    android:text="@string/registeronly"
                    android:textColor="@color/black" />

                <!--TextView
                    android:id="@+id/textViewVersion"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="10sp"
                    android:fontFamily="Georgia"
                    android:text="@string/desc"
                    android:gravity="center_horizontal"/-->

            </LinearLayout>
        </LinearLayout>


    </android.support.v7.widget.CardView>
    <!-- Second cardView starts-->
    <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
